{
  "name": "blockex",
  "version": "0.0.1",
  "description": "Cryptocurrency block explorer",
  "main": "./public",
  "repository": "git@github.com:dustinengle/blockex.git",
  "author": "Dustin Engle <dustin@dustinengle.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn run build:web",
    "build:web": "webpack --progress --colors --mode production",
    "cron:block": "./script/cron_block.sh >> ./tmp/block.log",
    "cron:coin": "node ./cron/coin.js >> ./tmp/coin.log",
    "cron:masternode": "node ./cron/masternode.js >> ./tmp/masternode.log",
    "cron:peer": "node ./cron/peer.js >> ./tmp/peer.log",
    "cron:rich": "node ./cron/rich.js >> ./tmp/rich.log",
    "cron:seed": "node ./cron/seed.js >> ./tmp/seed.log",
    "start:api": "NODE_ENV=production node ./server/index.js",
    "start:maint": "NODE_ENV=production node ./server/maintenance.js",
    "win:api": "node ./server/index.js",
    "win:maint": "node ./server/maintenance.js",
    "win:build": "webpack --progress --colors --config webpack_dev.config.js --mode development",
    "win:web": "webpack-dev-server --colors --config webpack_dev.config.js --progress  --mode development",
    "start:web": "webpack-dev-server --progress --colors --mode development",
    "test:api": "mocha --compilers js:babel-core/register --timeout 2000 ./test/server/api",
    "test:file": "mocha --compilers js:babel-core/register --timeout 2000 ./test/server/file",
    "test:rpc": "mocha --compilers js:babel-core/register --timeout 2000 ./test/server/rpc",
    "test:server": "yarn run test:api && yarn run test:file && yarn run test:rpc",
    "test:web": "mocha --compilers js:babel-core/register ./test/client"
  },
  "dependencies": {
    "split":"^1.0.1",
    "react-google-charts":"^3.0.10",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.7.3",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-rate-limit": "^3.3.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "mongoose": "^5.3.9",
    "morgan": "^1.9.1",
    "node-bitcoin-rpc": "^1.1.3",
    "numeral": "^2.0.6",
    "p-iteration": "^1.1.7",
    "prop-types": "^15.6.2",
    "qrcode": "^1.3.0",
    "react": "^16.6.0",
    "react-countup": "^4.0.0",
    "react-dom": "^16.6.0",
    "react-opium-select": "^0.5.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "reactstrap": "next",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "split":"^1.0.1",
    "animate-sass": "^0.8.2",
    "babel-cli": "^6.26.0",
    "react-google-charts":"^3.0.10",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.0.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "worker-loader": "^2.0.0",
    "webpack-bundle-analyzer" :"3.0.3"
  }
}
